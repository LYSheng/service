<section class="stayout_filtrate" style="display:none">
    <div class="text">按时间筛选</div>
    <div class="input">
        <input type="date" class="bign_time" id="beginTime" placeholder="开始时间"><span class="dx"></span><input type="date" placeholder="截止时间" class="end_time" id="endTime">
    </div>
    <div class="btn">
        <a href="javascript:void(0)" class="btnReset" id="btnReset">重置</a>
        <a href="javascript:void(0)" class="btnSuccess" id="btnSuccess">完成</a>
    </div>
</section>
<div id="mask"></div>

@section Scripts
