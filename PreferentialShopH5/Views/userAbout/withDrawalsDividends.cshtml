@model FrontSDK.Domain.DxlqOrderPackageCount
@{
    ViewBag.Title = "提现";
    Layout = "../Shared/_LayoutNone.cshtml";
}
@section Head
{
    @WebHelper4H5.RegisterStyles("userAbout/mine.css")
    <style>
        .syserror {
            color: #343333;
        }

        .notFound {
            position: absolute;
            top: 50%;
            left: 50%;
            margin-left: -2.25rem;
            margin-top: -2.8rem;
        }
    </style>
}
<div class="white-bg fs14 mt5 pl30 pr30">
    <p class="comAdrArea parbox">
        <label>储蓄卡</label>
        <input class="flex1 addusername disblock" type="text" value="" id="cardDetail" data-bind="value: withdrawalsCard,attr:{'data-bankcardid':bankCardId,'data-bankcardnum':bankCardNum},click:withCardList" placeholder="请选择银行卡" readonly>
    </p>
    <p class="comAdrArea parbox">
        <label>金额(元)</label>
        <input class="flex1 addiphone disblock" type="text" value="" id="newPwd" maxlength="16" data-bind="value: withdrawalsAmount" placeholder='可提现金额 @ViewBag.Account["canWithdrawDepositMoney"]元'>
    </p>
    <p class="comAdrArea parbox">
        <label>密码</label>
        <input class="flex1 addiphone disblock" type="password" value="" id="confirmPwd" maxlength="16" data-bind="value: withdrawalsPwd" placeholder="请输入支付密码">
    </p>
</div>
@if (ViewBag.IsCanWithDraw)
{
    <p class="pt4 pb2 pl20 pr20 fs14">最低提现金额：<i>@ViewBag.Condition["minWithDrawAmount"]</i>元</p>
    <p class="pt4 pb2 pl20 pr20 fs14">提现扣除手续费比率：<i>@((ViewBag.Condition["withDrawRate"] * 100).ToString("0.######"))</i>%</p>
    <p class="pt4 pb2 pl20 pr20 fs14">手续费封顶额：<i>@ViewBag.Condition["rateLimit"]</i>元</p>
    <p class="pt4 pb2 pl20 pr20 fs14">可提现次数：<i>@(ViewBag.Condition["withDrawCycleName"])</i></p>
    <p class="adraddBtn ac fs16" data-bind="click:confirm">提交</p>
}
@*else
{
    <p class="pt4 pb2 pl20 pr20 fs14" style="color:red">@ViewBag.ErrorMsg</p>
}*@
<div class="_payPwd" id="_payPwd">
    <div class="in">
        <div class="alert_title">
            <p class="fs14">为了您的账户资金安全，请设置支付密码。</p>
        </div>
        <div class="alert_btns parbox">
            <a href="javascript:void(0)" class="alert_btn alert_calcle flex1 fs14" id="payPwdCancle">稍后设置</a>
            <a href="/userAbout/payPwd?type=1&backurl=1" class="alert_btn alert_save flex1 fs14" id="payPwdSave">前去设置</a>
        </div>
    </div>
</div>
@*<div class="notFound">
        <p class="ac pb2"><img src="/Content/images/404.png" style="width:4.5rem" /></p>
        <div class="syserror ac fs16">暂无此页面</div>
    </div>*@

@section Footer
{

}
@section Scripts
{
    @WebHelper4H5.RegisterScripts("Account/withDrawalsDividends.js")
}
