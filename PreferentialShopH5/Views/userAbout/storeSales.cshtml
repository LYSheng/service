@{
    ViewBag.Title = "门店销售详情";
    Layout = "../Shared/_LayoutNone.cshtml";
}
@section Head
{
    @WebHelper4H5.RegisterStyles("userAbout/businessFeedBack.css")
}

<div class="storeSales white-bg" >
    <div class="store-desc parbox">
        <p>月度</p>
        <p class="flex1">实际销售额<span>(元)</span></p>
    </div>
    <ul data-bind="foreach:salesList">
        <li class="store-detail parbox">
            <p data-bind="text:year+'/'+month"></p>
            <p class="flex1" data-bind="text:amount.toFixed(2)"></p>
        </li>
    </ul>
</div>
<div class="members-btn">
    <a href="javascript:void(0)" class="ac white-bg"><span class="members-btn-icon"></span>查看<span id="year">2016</span>年详情</a>
</div>
<div id="proList"></div>
@section Scripts
{
    @WebHelper4H5.RegisterScripts("userAbout/storeSales.js")
}








